# .env

# Port untuk server Node.js
PORT=8081

# URL untuk API endpoint pengaturan background
API_BASE_URL=https://api.tiktok.com

# Client Key dan Secret dari TikTok Developer Portal
CLIENT_KEY=your_client_key_here
CLIENT_SECRET=your_client_secret_here

# URL untuk halaman redirect setelah otorisasi
REDIRECT_URI=http://localhost:8081/tiktok/callback

# Kunci untuk mengakses API atau layanan eksternal (misalnya, jika ada)
API_KEY=your_api_key_here
